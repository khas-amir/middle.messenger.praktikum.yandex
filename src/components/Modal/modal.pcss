@import "../../../static/css/mixins.pcss";

.modal {
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.4);
    position: absolute;
    top: 0;
    left: 0;
    display: none;

    &__content {
        background: var(--white-color);
        padding: 10px;
        @mixin center;
    }
}