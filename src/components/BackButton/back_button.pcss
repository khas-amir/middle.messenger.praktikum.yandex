.back {
    background-color: #FBFBFB;
    position: fixed;
    top: 0;
    left: 0;
    height: 100%;
    display: flex;
    align-items: center;
    border: 1px solid var(--gray-color-100);

    &__button {
        background-color: var(--primary-color);
        border-radius: 500%;
        width: 28px;
        height: 28px;
        display: flex;
        justify-content: center;
        align-items: center;
        margin: 0 18px;
        cursor: pointer;
    }
}